{
  "name": "iserranodev/encrypt-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony bundle for encrypting entity properties in Database",
  "keywords": ["symfony", "bundle", "encryption", "doctrine"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ismael Serrano LÃ³pez",
      "email": "iserrano.developer@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.0|^7.0",
    "beberlei/doctrineextensions": "^1.4",
    "doctrine/doctrine-bundle": "^2.10",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.16",
    "paragonie/halite": "^5.0",
    "symfony/console": "^6.0|^7.0",
    "symfony/filesystem": "^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "ISerranoDev\\EncryptBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ISerranoDev\\EncryptBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^6.0|^7.0"
  },
  "scripts": {
    "pre-package-uninstall": [
      "ISerranoDev\\EncryptBundle\\Composer\\ScriptHandler::removeConfig"
    ]
  }
}